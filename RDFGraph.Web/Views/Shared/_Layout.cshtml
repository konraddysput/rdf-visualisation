<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Wizualizacja RDF</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Content/toastr.min.css")" rel="stylesheet" />
</head>
<body>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>ZTI- Wizualizacja RDF - 2016/2017</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Scripts/lodash.js")"></script>
    <script src="@Url.Content("~/Scripts/cytoscape.js")"></script>
    <script src="@Url.Content("~/Scripts/scripts.js")"></script>
    <script src="@Url.Content("~/Scripts/toastr.min.js")"></script>
</body>
</html>
